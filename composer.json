{
    "name": "wallacemaxters/filament-image-color-picker",
    "description": "A custom Form Component to pick color from image",
    "license": "MIT",
    "keywords": [
        "wallacemaxters",
        "laravel",
        "filament",
        "image color picker",
        "color picker"
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "~3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },

    "autoload": {
        "psr-4": {
            "WallaceMaxters\\FilamentImageColorPicker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WallaceMaxters\\FilamentImageColorPicker\\ImageColorPickerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
